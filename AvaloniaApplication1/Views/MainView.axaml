<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:AvaloniaApplication1.ViewModels"
        xmlns:managers="clr-namespace:MFUtility.Mvvm.Avalonia.Managers;assembly=MFUtility.Mvvm.Avalonia"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaApplication1.Views.MainView"
        x:DataType="vm:MainViewModel"
        Title="MainView">
    <Grid ColumnDefinitions="*,*">
        <StackPanel Spacing="10">
            <Button Content="GoHome" Command="{Binding GoHomeCommand}"/>
            <Button Content="GoAbout" Command="{Binding GoAboutCommand}"/>
            <Button Content="GoBack" Command="{Binding GoBackCommand}"/>
        </StackPanel>
        <ContentControl Grid.Column="1" managers:RegionManagerAttached.RegionName="Main" />
    </Grid>
</Window>